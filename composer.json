{
    "name": "onwwward/lumen_consuming-restful-web-apis",
    "description": "Example application in Lumen for consuming restful web apis",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "mpclarkson/freshdesk-laravel": "^0.2.2",
        "onwwward/laravel-bugherd": "^0.1.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
	    "laravel/homestead": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
